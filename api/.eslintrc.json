{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "prettier/@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "class-methods-use-this": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".ts", ".js"]
      }
    }
  }
}
